{
  "name": "marvel-content-grabber",
  "version": "1.0.0",
  "description": "A Marvel API content grabber",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./config/webpack.prod.js",
    "dev": "yarn run db | webpack-dev-server --config ./config/webpack.dev.js",
    "debug": "node $NODE_DEBUG_OPTION node_modules/.bin/webpack-dev-server --config ./config/webpack.dev.js",
    "db": "json-server --watch db.json",
    "start": "node server/index.js",
    "test": "mocha || true"
  },
  "repository": "https://github.com/FabioAnsaldi/marvel-content-grabber.git",
  "author": "Fabio Ansaldi",
  "license": "MIT",
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.16.4"
  }
}
